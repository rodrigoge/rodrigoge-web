{
  "extends": ["next/core-web-vitals", "next", "next/typescript", "prettier"],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "semi": false,
        "singleQuote": true,
        "tabWidth": 2
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "warn",
    "import/no-unresolved": "error",
    "import/named": "error"
  }
}
